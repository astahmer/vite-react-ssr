{
    "name": "vite-chakra-ssr",
    "version": "0.0.0",
    "scripts": {
        "dev": "node -r @swc-node/register -r tsconfig-paths/register ./src/server",
        "build:server": "vite build && vite build --ssr",
        "build:ssg": "vite-plugin-ssr prerender",
        "build": "yarn build:server && yarn build:ssg",
        "serve": "vite preview",
        "prettier": "prettier --write \"./{src,tests}/**/*\""
    },
    "dependencies": {
        "@chakra-ui/icons": "^1.0.15",
        "@chakra-ui/react": "^1.6.6",
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@pastable/core": "^0.1.9",
        "@xstate/react": "^1.5.1",
        "axios": "^0.21.4",
        "express": "^4.17.1",
        "framer-motion": "^4.1.17",
        "jotai": "^1.2.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.11.1",
        "react-icons": "^4.2.0",
        "react-query": "^3.19.4",
        "react-router-dom": "^5.2.0",
        "tsconfig-paths": "^3.11.0",
        "vite-plugin-ssr": "^0.3.1",
        "xstate": "^4.23.0"
    },
    "devDependencies": {
        "@swc-node/register": "^1.3.5",
        "@types/express": "^4.17.12",
        "@types/node": "^15.12.1",
        "@types/react": "^17.0.9",
        "@types/react-dom": "^17.0.6",
        "@vitejs/plugin-react-refresh": "^1.3.3",
        "prettier": "^2.4.0",
        "prettier-plugin-sorted": "^2.0.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.3.2",
        "vite": "^2.4.3",
        "vite-plugin-pwa": "^0.10.0",
        "vite-react-jsx": "^1.1.2"
    },
    "importSort": {
        ".js, jsx, .ts, .tsx": {
            "options": {
                "ignoreTsConfig": true,
                "extraAliases": [
                    "@/*"
                ],
                "bottomAliases": [
                    "/assets"
                ]
            }
        }
    }
}
