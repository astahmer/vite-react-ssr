{
    "name": "vite-chakra",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "prettier": "prettier --write \"./{src,tests}/**/*\""
    },
    "dependencies": {
        "@chakra-ui/react": "^1.6.5",
        "@emotion/react": "^11",
        "@emotion/styled": "^11",
        "@pastable/core": "^0.1.9",
        "@xstate/react": "^1.5.1",
        "axios": "^0.21.1",
        "framer-motion": "^4",
        "jotai": "^1.2.2",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-hook-form": "^7.11.1",
        "react-icons": "^4.2.0",
        "react-query": "^3.19.4",
        "react-router-dom": "^5.2.0",
        "xstate": "^4.23.0"
    },
    "devDependencies": {
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@vitejs/plugin-react-refresh": "^1.3.1",
        "prettier": "^2.4.0",
        "prettier-plugin-sorted": "^2.0.0",
        "typescript": "^4.3.2",
        "vite": "^2.4.3",
        "vite-plugin-pwa": "^0.8.2",
        "vite-react-jsx": "^1.1.2"
    },
    "importSort": {
        ".js, jsx, .ts, .tsx": {
            "options": {
                "ignoreTsConfig": true,
                "extraAliases": [
                    "@/*"
                ],
                "bottomAliases": [
                    "/assets"
                ]
            }
        }
    }
}
